{
  "manifest_version": 3,
  "name": "Promptin",
  "version": "1.0",
  "description": "A Chrome extension that helps you manage your prompts",
  "permissions": [
    "activeTab"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "URL Tracker Extension"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
} 